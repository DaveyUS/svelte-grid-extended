{
	"name": "svelte-grid-extended",
	"version": "0.3.0",
	"description": "A draggable and resizable grid layout, for Svelte",
	"repository": "https://github.com/cuire/svelte-grid-extended",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"test": "vitest",
		"coverage": "vitest run --coverage",
		"e2e": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "husky install"
	},
	"keywords": [
		"svelte",
		"grid",
		"drag",
		"draggable",
		"resize",
		"resizable",
		"fluid",
		"responsive"
	],
	"author": "cuire <garwes@icloud.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/cuire/svelte-grid-extended/issues"
	},
	"devDependencies": {
		"@playwright/test": "^1.25.0",
		"@sveltejs/adapter-auto": "^1.0.0-next.75",
		"@sveltejs/kit": "^1.0.0-next.490",
		"@sveltejs/package": "next",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@vitest/coverage-c8": "^0.23.4",
		"autoprefixer": "^10.4.12",
		"daisyui": "^2.33.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.18",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.50.1",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.1",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0",
		"vitest": "^0.23.4",
		"husky": "^8.0.0"
	},
	"type": "module"
}
